# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'Subtela' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  # Pods for Subtela
  pod 'Instructions', '~> 1.3.1'
  pod 'RealmSwift' # persistent data
  pod 'Timepiece' # Dates are a bitch
  pod 'BulletinBoard' # card popups
  pod 'PKRevealController' # Initial how-to screen

end
